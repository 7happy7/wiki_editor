{
    "name": "Wiki Editor",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "CHROME EXTENSION FOR EDITING YOUR WIKI PAGES.",
    "author": "7happy7",
    "permissions": ["storage", "*://*.scp-wiki.net/*", "*://*.wikidot.com/*"],
    "web_accessible_resources": ["source/*.json", "editor/*.js"],
    "content_scripts": [
        {
            "matches": ["*://*.scp-wiki.net/*", "*://*.wikidot.com/*"],
            "js": ["editor/editor.js"],
            "run_at": "document_end",
            "all_frames": true
        }
    ]
}
